# Frontend Environment Variables Template

# API URLs - Update these for your deployment
VITE_API_BASE_URL=http://localhost:10000
VITE_PYTHON_OMR_URL=http://localhost:5000

# Production URLs (uncomment and update for production)
# VITE_API_BASE_URL=https://your-backend.onrender.com
# VITE_PYTHON_OMR_URL=https://your-python-omr.onrender.com

# App Configuration
VITE_APP_NAME=EvalBee OMR Scanner
VITE_APP_VERSION=2.0.0

# Debug Settings
VITE_DEBUG_MODE=true
VITE_ENABLE_CONSOLE_LOGS=true

# Camera Settings
VITE_CAMERA_QUALITY_THRESHOLD=0.8
VITE_AUTO_CAPTURE_DELAY=3000

# File Upload Settings
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/jpg